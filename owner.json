const axios = require("axios");
const cheerio = require("cheerio");

async function extractDownloadLink() {
    const pageUrl = "https://www.lulacloud.com/d/hXVNtYnAu14-sonic-the-hedgehog-3-2024-awafilm-tv-mkv";

    try {
        // Fetch the page HTML with headers
        const response = await axios.get(pageUrl, {
            headers: {
                "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36",
                "Referer": "https://www.lulacloud.com/",
            },
        });

        // Load the page HTML
        const $ = cheerio.load(response.data);

        // Debugging HTML content (optional)
        console.log("Page HTML Loaded Successfully.");

        // Find the specific link with "Download" text
        const downloadLink = $("a")
            .filter((_, el) => $(el).text().includes("Download"))
            .attr("href");

        if (downloadLink) {
            console.log("Download Link Found:", downloadLink);
        } else {
            console.log("No download link found on the page.");
        }
    } catch (error) {
        console.error("Error extracting download link:", error.message);
    }
}

// Run the function
extractDownloadLink();
